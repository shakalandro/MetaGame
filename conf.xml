<?xml version="1.0" encoding="UTF-8" ?>
<Module>
  <ModulePrefs title="MetaGame">
    <Require feature="rpc"/>
    <Require feature="views"/>
  </ModulePrefs>
  <Content type="html">
    <![CDATA[
      <script src="//hangoutsapi.talkgadget.google.com/hangouts/api/hangout.js?v=0.1"></script>
      <script src="http://thebeast.hopto.org/eric/swfobject.js"></script>
      <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.js"></script>
      <script src="http://thebeast.hopto.org/roy/metagame.js"></script>
      <link type="text/css" rel="stylesheet" href="http://thebeast.hopto.org/roy/metagame.css" />
      <link href='http://fonts.googleapis.com/css?family=Cabin+Sketch' rel='stylesheet' type='text/css'>
      <div id="app_body">
        <div id="game_outer">
            <div id="game"></div>
        </div>
        <div id="content_wrapper">
            <h1>MetaGame</h1>
            <div id="app_content"></div>
            <div id="scores_pane"></div>
        </div>
        <p>For best results, submit all scores!</p>
      </div>
      <div id="leaderboard_bridge"></div>
      <script src="http://xs.mochiads.com/static/pub/swf/leaderboard.js" type="text/javascript"></script>
    ]]>
  </Content>
</Module>
